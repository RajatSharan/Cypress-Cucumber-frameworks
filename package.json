      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "author": "Rajat sharan",
  "license": "ISC",
  "devDependencies": {
    "cypress-cucumber-preprocessor": "^4.3.1",
    "multiple-cucumber-html-reporter": "^1.21.2"
  },
  "dependencies": {
    "cypress": "^10.0.1"
  }
}
